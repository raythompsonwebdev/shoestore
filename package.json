{
  "name": "shoestore",
  "version": "0.2.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@csstools/postcss-sass": "^5.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "autoprefixer": "^10.4.13",
    "bootstrap": "^5.2.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fibers": "^5.0.3",
    "flow-bin": "^0.198.2",
    "history": "^5.3.0",
    "node-sass": "^8.0.0",
    "postcss": "^8.4.21",
    "postcss-preset-env": "^8.0.1",
    "prop-types": "^15.8.1",
    "react-bootstrap": "^2.7.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.58.0",
    "serve-static": "^1.15.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "terser": "^5.16.2",
    "util": "^0.12.5",
    "web-vitals": "^3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "server.js",
    "flow": "flow",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\"",
    "watch:css": "postcss src/static/sass/style.css -o src/static/css/style.css -w",
    "build:css": "postcss src/static/sass/style-copy.scss -o src/static/css/style.css",
    "stylelint:scss": "npx stylelint \"src/static/sass/style-copy.scss\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3"
  }
}
